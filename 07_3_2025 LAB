CREATE TABLE authors (
AUTHOR_ID INT,
FNAME VARCHAR(100) ,
LNAME VARCHAR(100),
BIO VARCHAR(200)
DOB DATE
));

CREATE TABLE publishers(
PUB_ID INT,
PUBNAME VARCHAR(100),
COUNTRY VARHCAR(100)
));

CREATE TABLE books(
BOOK_ID INT,
BOOK_TITLE VARCHAR(100),
ISBN INT,
PUBLISHED DATETIME,
PRICE DOUBLE(00,00)
));
